{
  "name": "codes",
  "version": "0.0.1",
  "description": "source code for ayokoding",
  "main": "index.js",
  "scripts": {
    "init:js": "cd js && yarn install",
    "clean:js": "cd js && yarn clean",
    "reset:js": "cd js && yarn reset",
    "init:codes": "yarn install",
    "clean:codes": "rm -rf node_modules",
    "reset:codes": "yarn clean:codes && yarn init:codes",
    "init:package": "yarn init:codes && yarn init:js",
    "clean:package": "yarn clean:codes && yarn clean:js",
    "reset:package": "yarn clean:package && yarn init:package"
  },
